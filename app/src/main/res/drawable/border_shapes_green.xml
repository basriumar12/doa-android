<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid
                android:color="@color/colorVeryLightPink_c1" />
            <size
                android:width="150dp"
                android:height="150dp"/>
        </shape>
    </item>
    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="oval">
            <solid
                android:color="@color/colorWhite" />
            <size
                android:width="150dp"
                android:height="150dp"/>
        </shape>
    </item>
</layer-list>